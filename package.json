    "@typescript-eslint/eslint-plugin": "^6.1.0",
    "@typescript-eslint/parser": "^6.1.0",
    "@vitejs/plugin-vue": "^4.2.3",
    "@vue/tsconfig": "^0.4.0",
    "eslint": "^8.45.0",
    "eslint-plugin-vue": "^9.15.1",
    "npm-run-all": "^4.1.5",
    "sass": "^1.63.6",
    "sass-loader": "^13.3.2",
    "typescript": "~5.0.4",
    "vite": "^4.3.9",
    "vue-template-compiler": "^2.7.14",
    "vue-tsc": "^1.6.5"
  }
